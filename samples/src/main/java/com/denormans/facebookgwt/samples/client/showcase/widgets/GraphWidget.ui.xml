<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:sc="urn:import:com.denormans.facebookgwt.samples.client.showcase.widgets">
  <g:HTMLPanel>
    <div class="FBGWTField">
      <g:Button ui:field="retrieveCurrentUserButton">Get Current User</g:Button>
      <g:Button ui:field="retrieveHomeFeedButton">Home Feed</g:Button>
      <g:Button ui:field="retrieveWallFeedButton">Wall Feed</g:Button>
      <g:Button ui:field="retrieveFriendsButton">Friends</g:Button>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="postToWallButton">Post to Home Feed</g:Button>
      <g:TextBox ui:field="postToWallMessageTextBox"/>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="deletePostButton">Delete a Post</g:Button>
      <g:TextBox ui:field="deletePostIDTextBox"/>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="likeItemButton">Like an item</g:Button>
      <g:TextBox ui:field="likeItemIDTextBox"/>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="unlikeItemButton">Unlike an item</g:Button>
      <g:TextBox ui:field="unlikeItemIDTextBox"/>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="testButton">Test</g:Button>
      <g:TextBox ui:field="testTextBox"/>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>