<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HTMLPanel>
    <div class="FBGWTField">
      <g:Button ui:field="bookmarkButton" enabled="false">Bookmark Application</g:Button>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="streamPublishButton" enabled="false">Publish to Stream</g:Button>
      <g:TextBox ui:field="streamPublishMessageTextBox" enabled="false">Learning about FacebookGWT</g:TextBox>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="streamShareButton" enabled="false">Share Link to Stream</g:Button>
      <g:TextBox ui:field="streamShareLinkTextBox" enabled="false">http://denormans.github.com/FacebookGWT</g:TextBox>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="friendAddButton" enabled="false">Add Friend</g:Button>
      <g:TextBox ui:field="friendAddIDTextBox" enabled="false"/>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>