<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HTMLPanel>
    <div class="FBGWTField">
      <g:Button ui:field="enableAutoResizeButton">Enable Auto Resize</g:Button>
      <g:Button ui:field="disableAutoResizeButton">Disable Auto Resize</g:Button>
    </div>

    <div class="FBGWTField">
      <g:Button ui:field="setSizeButton">Set Size</g:Button>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>