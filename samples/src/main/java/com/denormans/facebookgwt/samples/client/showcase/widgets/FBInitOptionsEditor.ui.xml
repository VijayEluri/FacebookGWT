<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HTMLPanel styleName="FBGWTEditor">
    <div class="FBGWTHeader">Options:</div>

    <div class="FBGWTContent">
      <g:Label>Application ID</g:Label>
      <g:TextBox ui:field="applicationIDEditor"/>

      <g:CheckBox ui:field="enableCookieSupportEditor">Enable Cookie Support</g:CheckBox>
      <g:CheckBox ui:field="enableLoggingEditor">Enable Logging</g:CheckBox>
      <g:CheckBox ui:field="fetchStatusEditor">Fetch Status</g:CheckBox>
      <g:CheckBox ui:field="parseXFBMLTagsEditor">Parse XFBML Tags</g:CheckBox>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>