<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN" "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="FacebookGWTSamples">
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.denormans.facebookgwt.api.FacebookGWTAPI'/>

  <set-property name="gwt.logging.logLevel" value="FINE"/>
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <set-property name="gwt.logging.popupHandler" value="DISABLED"/>

  <entry-point class='com.denormans.facebookgwt.samples.client.FacebookGWTSamples'/>
</module>
